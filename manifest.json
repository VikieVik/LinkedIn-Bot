{
  "manifest_version": 2,
  "name": "linkedin-connection-bot",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*", "http://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "./icons/favicon-32x32.png",
    "default_popup": "./popup/popup.html",
    "default_title": "Linkedin Connection Bot"
  }
}
